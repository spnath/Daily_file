   "source": [
    "# Remove Punctuations from strings"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "9519a2b8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter Any Number of Here: hi>> thid*** great time@@@@\n",
      "hi thid great time\n"
     ]
    }
   ],
   "source": [
    "while True:\n",
    "    punc='''!@#$%^&*()_+<>??|\\|/`~'''\n",
    "\n",
    "    string=input('Enter Any Number of Here: ')\n",
    "\n",
    "    empty_str=\"\"\n",
    "\n",
    "    for i in string:\n",
    "        if i not in punc:\n",
    "            empty_str+= i\n",
    "    #         emtpy_str=empty_str+i\n",
    "\n",
    "    print(empty_str)"
   ]
